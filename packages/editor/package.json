{
  "name": "graphql-editor",
  "version": "7.2.16",
  "private": false,
  "license": "MIT",
  "description": "Visual node editor for GraphQL",
  "homepage": "https://graphqleditor.com",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/cjs/index.d.ts",
  "files": [
    "lib/cjs",
    "lib/esm"
  ],
  "scripts": {
    "build:cjs": "ttsc --build tsconfig.cjs.json",
    "build:esm": "ttsc --build tsconfig.esm.json",
    "build": "rimraf lib tsconfig.build.tsbuildinfo && npm run build:esm && npm run build:cjs",
    "start": "npm run build -- --watch",
    "test": "jest",
    "updatedeps": "npm i graphql-js-tree@latest",
    "lint": "eslint \"./src/**/*.{ts,js}\" --quiet --fix && prettier --write \"./src/**/*.{ts,js}\" "
  },
  "resolutions": {
    "@types/react": "^16.9.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/graphql-editor/graphql-editor.git"
  },
  "bugs": {
    "url": "https://github.com/graphql-editor/graphql-editor.git"
  },
  "peerDependencies": {
    "@monaco-editor/react": ">=4.2.2",
    "css-loader": ">=6.2.0",
    "file-loader": ">=6.2.0",
    "react": ">=18",
    "react-dom": ">=18",
    "webpack": ">=5.53.0",
    "worker-loader": ">=3.0.8",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3"
  },
  "dependencies": {
    "@aexol-studio/hooks": "0.2.8",
    "@aexol-studio/styling-system": "0.2.21",
    "monaco-editor": ">=0.27.0",
    "color2k": "^1.2.4",
    "d3": "^7.8.4",
    "diff": "^5.1.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^10.12.16",
    "fuzzyjs": "^5.0.1",
    "graphql-editor-worker": "^7.2.14",
    "graphql-js-tree": "^3.0.1",
    "graphql-language-service": "3.2.5",
    "html-to-image": "^1.10.8",
    "re-resizable": "^6.9.1",
    "react-laag": "^2.0.4",
    "react-zoom-pan-pinch": "^3.0.7",
    "remarkable": "^2.0.1",
    "unstated-next": "^1.1.0",
    "yup": "^1.3.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/d3": "^7.4.0",
    "@types/diff": "^5.0.2",
    "rollup": "^4.19.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.36.0"
  }
}
