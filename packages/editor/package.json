  },
  "peerDependencies": {
    "@monaco-editor/react": ">=4.2.2",
    "css-loader": ">=6.2.0",
    "file-loader": ">=6.2.0",
    "monaco-editor": ">=0.27.0",
    "react": ">=18",
    "react-dom": ">=18",
    "webpack": ">=5.53.0",
    "worker-loader": ">=3.0.8"
  },
  "dependencies": {
    "@aexol-studio/hooks": "^0.2.8",
