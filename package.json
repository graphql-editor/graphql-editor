{
  "name": "graphql-editor",
  "version": "5.4.6",
  "private": false,
  "license": "MIT",
  "description": "Visual node editor for GraphQL",
  "homepage": "https://graphqleditor.com",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "webpack serve --config ./sandbox/webpack.config.js --port 1568 --hot --open",
    "start-ci": "webpack serve --config ./sandbox/webpack.config.ci.js --port 1568 --hot --open",
    "build": "ttsc --build tsconfig.build.json",
    "watch": "ttsc --watch",
    "domTree": "tree src/domStructure/domStructure.treelang -f ts > src/domStructure/domStructure.ts",
    "test": "jest",
    "e2e:test": "cypress open",
    "updatedeps": "npm i graphql-zeus@latest graphsource@latest",
    "lint": "eslint \"./src/**/*.{ts,js}\" --quiet --fix && prettier --write \"./src/**/*.{ts,js}\" "
  },
  "resolutions": {
    "@types/react": "^16.9.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/graphql-editor/graphql-editor.git"
  },
  "bugs": {
    "url": "https://github.com/graphql-editor/graphql-editor.git"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@monaco-editor/react": "^4.3.1",
    "@types/classnames": "^2.3.0",
    "@types/faker": "^5.5.8",
    "@types/graphql": "^14.2.3",
    "@types/history": "4.7.9",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.2",
    "@types/query-string": "^6.2.0",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@types/remarkable": "^2.0.3",
    "@types/webpack": "5.28.0",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.1",
    "cypress": "^10.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.2",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^6.3.3",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "mocha": "^9.1.1",
    "monaco-editor": "^0.29.1",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "style-loader": "^3.2.1",
    "treelang": "0.0.4",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "ttypescript": "^1.5.12",
    "typedoc": "^0.22.3",
    "typescript": "^4.4.3",
    "typescript-transform-paths": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.53.0",
