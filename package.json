    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "ttypescript": "^1.5.12",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.3",
    "typescript-transform-paths": "^2.0.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.28.4",
    "webpack-cleanup-plugin": "^0.5.1",
