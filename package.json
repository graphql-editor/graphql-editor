  "dependencies": {
    "@emotion/styled": "^11.9.3",
    "classnames": "^2.3.1",
    "color2k": "^1.2.4",
    "fuzzyjs": "^5.0.1",
    "graphql-js-tree": "^0.0.3",
    "graphql-language-service": "^3.1.4",
    "graphsource": "^2.0.5",
    "re-resizable": "^6.9.1",
    "remarkable": "^2.0.1",
    "typestyle": "^2.1.0",
    "unstated-next": "^1.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
