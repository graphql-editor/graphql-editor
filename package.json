    "@typescript-eslint/parser": "^2.12.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^3.5.3",
    "cypress": "^4.5.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.0.0-alpha.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
